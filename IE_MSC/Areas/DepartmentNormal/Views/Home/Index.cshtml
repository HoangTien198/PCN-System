<!-- Department Normal -->
@using IE_MSC.Commons
@{
    Layout = null;
    var userLogin = (AccountLogin)Session["USER_SESSION"];
    var nameDisplay = "";
    if (userLogin.EmployeeVNName != null)
    {
        nameDisplay = (userLogin.EmployeeCNName + " - " + userLogin.EmployeeVNName);
    }
    else
    {
        nameDisplay = (userLogin.EmployeeCNName);
    }

}
@{
    ViewBag.Title = "Index";
    Layout = "~/Areas/DepartmentNormal/Views/Shared/_DepartmentNormal_View.cshtml";
}

<link href="~/Assets/MSC/library/simple-datatables/dataTableLibrary.css" rel="stylesheet" />
<link href="~/Assets/MSC/library/drag-and-drop-reorder-list/css/style.css" rel="stylesheet" />
<style>
    .dataTables_filter {
        float: left !important;
        text-align: left;
    }
    tr {
        vertical-align: baseline;
    }

    thead {
        position: sticky;
        top: -9px;
        background: #6c757d;
    }

    .tblLayout .tab-content {
        height: calc(100vh - 420px);
        overflow: auto;
    }

    .dropdown-menu .dropdown-item:hover {
        background-color: #5F94C2;
    }
</style>
@*<link href="~/Assets/MSC/library/DataTable/dataTables.bootstrap.css" rel="stylesheet" />
    <link href="~/Assets/MSC/library/DataTable/dataTables.responsive.css" rel="stylesheet" />*@
<link href="~/Assets/MSC/library/jQuery-TE_v.1.4.0/jquery-te-1.4.0.css" rel="stylesheet" />

<div class="row">
    <div class="col-md-3">
        <div class="card mb-3">
            <div class="row g-0" onclick="SetTrigger('home-tab')">
                <div class="col-md-4 d-flex justify-content-center align-items-center bg-primary" style="font-size: 65px;">
                    <i class="bi bi-clock-history text-white"></i>
                </div>
                <div class="col-md-8 d-flex justify-content-center align-items-center">
                    <div class="card-body text-center">
                        <h2 class="text-primary waitingToSignQty">0</h2>
                        <p class="card-text">My application</p>
                    </div>
                </div>
            </div>
        </div>
    </div>
    <div class="col-md-3">
        <div class="card mb-3">
            <div class="row g-0" onclick="SetTrigger('profile-tab1');return false">
                <div class="col-md-4 d-flex justify-content-center align-items-center bg-warning" style="font-size: 65px;">
                    <i class="bi bi-hourglass-split text-white"></i>
                </div>
                <div class="col-md-8 d-flex justify-content-center align-items-center">
                    <div class="card-body text-center">
                        <h2 class="text-warning waitingForApprovalQty">0</h2>
                        <p class="card-text">Waiting For Approval</p>
                    </div>
                </div>
            </div>
        </div>
    </div>
    <div class="col-md-3">
        <div class="card mb-3">
            <div class="row g-0" onclick="SetTrigger('profile-tab')">
                <div class="col-md-4 d-flex justify-content-center align-items-center bg-success" style="font-size: 65px;">
                    <i class="bi bi-list-check text-white"></i>
                </div>
                <div class="col-md-8 d-flex justify-content-center align-items-center">
                    <div class="card-body text-center">
                        <h2 class="text-success approvedQty">0</h2>
                        <p class="card-text">Approved by me</p>
                    </div>
                </div>
            </div>
        </div>
    </div>
    <div class="col-md-3">
        <div class="card mb-3">
            <div class="row g-0" onclick="SetTrigger('contact-tab')">
                <div class="col-md-4 d-flex justify-content-center align-items-center bg-danger" style="font-size: 65px;">
                    <i class="bi bi-x-circle text-white"></i>
                </div>
                <div class="col-md-8 d-flex justify-content-center align-items-center">
                    <div class="card-body text-center">
                        <h2 class="text-danger rejectedQty">0</h2>
                        <p class="card-text">Rejected by me</p>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>

<div class="row tblLayout">
    <div class="card">
        <div class="card-header">
            @*<h1>List MSC</h1>*@
            <button id="addMSC" type="button" class="btn btn-primary" data-bs-toggle="modal" data-bs-target="#add-msc-modal">
                <i class="ri-add-circle-line"></i> Tạo đơn
            </button>
            @*<input id="searchMSC" type="text" name="query" placeholder="Search" title="Enter search keyword">*@
        </div>
        <div class="card-body">
            <!-- Bordered Tabs -->
            <ul class="nav nav-tabs nav-tabs-bordered" id="borderedTab" role="tablist">
